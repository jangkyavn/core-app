                                        <h4><a href="@url" class="blog_title">@item.Name</a></h4>
                                        <div class="jtv-entry-meta">
                                            <i class="fa fa-clock-o"></i> <strong>@item.DateCreated.Value.ToString("d")</strong>
                                            <a href="#"><i class="fa fa-commenting-o"></i> <strong>15 Bình luận</strong></a>
