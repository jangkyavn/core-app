@model HomeViewModel

@inject IProductService ProductService
@inject IReviewService ReviewService

@{
    ViewData["Title"] = Model.Title;
    ViewData["MetaKeyword"] = Model.MetaKeyword;
    ViewData["MetaDescription"] = Model.MetaDescription;
}

@section Styles {
    <style type="text/css">
        .blog_title {
            display: -webkit-box;
            -webkit-line-clamp: 2;
            -webkit-box-orient: vertical;
            overflow: hidden;
            text-overflow: ellipsis;
        }

        .blog_description {
            display: -webkit-box;
            -webkit-line-clamp: 3;
            -webkit-box-orient: vertical;
            overflow: hidden;
            text-overflow: ellipsis;
        }
    </style>
}

<!-- Slideshow  -->
<div class="main-slider" id="home">
    <div class="container">
        <div class="row">
            <div class="col-md-3 col-sm-3 col-xs-12 banner-left hidden-xs">
                <img src="~/client-side/images/banner-left.jpg" alt="banner">
            </div>
            <div class="col-sm-9 col-md-9 col-lg-9 col-xs-12 jtv-slideshow">
                <div id="jtv-slideshow">
                    <div id='rev_slider_4_wrapper' class='rev_slider_wrapper fullwidthbanner-container'>
                        <div id='rev_slider_4' class='rev_slider fullwidthabanner'>
                            <ul>
                                @foreach (var slide in Model.HomeSlides)
                                {
                                    <li data-transition='fade' data-slotamount='7' data-masterspeed='1000' data-thumb=''>
                                        <img src='@slide.Image' data-bgposition='left top' data-bgfit='cover' data-bgrepeat='no-repeat' alt="@slide.Name" />
                                    </li>
                                }
                            </ul>
                            <div class="tp-bannertimer"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- service section -->
<partial name="~/Views/Shared/_ServicePartial.cshtml" />

<div class="inner-box">
    <div class="container">
        <div class="row">
            <!-- Banner -->
            <div class="col-md-3 top-banner hidden-sm">
                <div class="jtv-banner3">
                    <div class="jtv-banner3-inner">
                        <a href="#"><img src="~/client-side/images/sub1.jpg" alt="HTML template"></a>
                        <div class="hover_content">
                            <div class="hover_data">
                                <div class="title"> Giảm giá lớn</div>
                                <div class="desc-text">Giảm tới 55%</div>
                                <p><a href="#" class="shop-now">Mua ngay!</a></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Best Sale -->
            <div class="col-sm-12 col-md-9 jtv-best-sale special-pro">
                <div class="jtv-best-sale-list">
                    <div class="wpb_wrapper">
                        <div class="best-title text-left">
                            <h2>
                                Ưu đãi đặc biệt
                            </h2>
                        </div>
                    </div>
                    <div class="slider-items-products">
                        <div id="jtv-best-sale-slider" class="product-flexslider">
                            <div class="slider-items">
                                @foreach (var item in Model.PromotionProducts)
                                {
                                    var urlImage = $"/uploaded/images/products/{item.Image}";
                                    var url = $"/{item.SeoAlias}-p.{item.Id}.html";

                                    <div class="product-item">
                                        <div class="item-inner">
                                            <div class="product-thumbnail">
                                                @if (item.HotFlag)
                                                {
                                                    <div class="icon-sale-label sale-left">Hot</div>
                                                }
                                                @if ((DateTime.Now - item.DateCreated.Value).Days <= 7)
                                                {
                                                    <div class="icon-new-label new-right">Mới</div>
                                                }
                                                <div class="pr-img-area">
                                                    <a title="@item.Name" href="@url">
                                                        <figure>
                                                            <img class="first-img" src="@urlImage?w=190" alt="@item.Name">
                                                            <img class="hover-img" src="@urlImage?w=190" alt="@item.Name">
                                                        </figure>
                                                    </a>
                                                </div>
                                                <div class="pr-info-area">
                                                    <div class="pr-button">
                                                        <div class="mt-button add_to_wishlist">
                                                            <a href="wishlist.html">
                                                                <i class="fa fa-heart-o"></i>
                                                            </a>
                                                        </div>
                                                        <div class="mt-button add_to_compare">
                                                            <a href="compare.html">
                                                                <i class="fa fa-link"></i>
                                                            </a>
                                                        </div>
                                                        <div class="mt-button quick-view">
                                                            <a href="#" class="btn-quich-view" data-id="@item.Id">
                                                                <i class="fa fa-search"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="item-info">
                                                <div class="info-inner">
                                                    <div class="item-title">
                                                        <a title="@item.Name" href="@url">@item.Name </a>
                                                    </div>
                                                    <div class="item-content">
                                                        <div class="rating">
                                                            @for (int i = 1; i <= 5; i++)
                                                            {
                                                                if (i <= await ReviewService.GetRatingAverageAsync(item.Id))
                                                                {
                                                                    <i class="fa fa-star"></i>
                                                                }
                                                                else
                                                                {
                                                                    <i class="fa fa-star-o"></i>
                                                                }
                                                            }
                                                        </div>
                                                        <div class="item-price">
                                                            @if (item.PromotionPrice.HasValue)
                                                            {
                                                                <p class="special-price">
                                                                    <span class="price"> @item.PromotionPrice.Value.ToString("N0") </span>
                                                                </p>
                                                                <p class="old-price">
                                                                    <span class="price"> @item.Price.ToString("N0") </span>
                                                                </p>
                                                            }
                                                            else
                                                            {
                                                                <div class="price-box">
                                                                    <span class="regular-price">
                                                                        <span class="price">@item.Price.ToString("N0")</span>
                                                                    </span>
                                                                </div>
                                                            }
                                                        </div>
                                                        <div class="pro-action">
                                                            <button type="button" class="add-to-cart" onclick="window.location.href='@url'">
                                                                <span> Mua ngay</span>
                                                            </button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

